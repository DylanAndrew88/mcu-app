
  <!DOCTYPE html>
  <html>
    <head>
      <meta charset="utf-8">
      <link href="https://fonts.googleapis.com/css?family=Bangers|Francois+One|Quantico|Russo+One" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="/stylesheets/styles.css">
      <title>{{title}} - Home</title>
    </head>
    <body>
      <h1 class="page-title">{{title}}</h1>
      <h3 class="movies-header">Movies (2008-present)</h3>
        <div class="movies-display">
        {{#movies}}
          <div class="movie-select">
            <h4 class="movie-title"><a class="movie-title" href="/movies/{{link}}">{{title}} ({{year}})</a></h4>
            <img class="poster-home" src="/images/{{link}}.jpg">
          </div>
        {{/movies}}
        </div>
    </body>
  </html>
